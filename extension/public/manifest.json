{
    "manifest_version": 3,
    "version": "1.0.0",
    "name": "Cart Guardian",
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
}